<rdf:RDF
xmlns:dc="&dc;"
xmlns:dcterms="&dcterms;"
xmlns:rdf="&rdf;"
xmlns:ore="&ore;"
>

<!-- URI-G -->
<ore:Graph rdf:about="http://www.example.org/graph/1">

  <!-- We attach metadata about the Named Graph to URI-G -->
  <dc:creator rdf:resource="http://example.org/person/PeteJ" />
  <dcterms:created>2007-05-29</dcterms:created>

  <ore:describes>

    <!-- URI-CO -->
    <ore:CompoundObject rdf:about="http://www.example.org/lom/1">

      <!-- We attach metadata about the Compound Object to URI-CO -->
      <dc:title>A Learning Object</dc:title>
      <dc:creator rdf:resource="http://example.org/person/LeighDodds" />
      <dcterms:created>2007-05-29</dcterms:created>

      <ore:hasParts>
        <rdf:Seq>
          <!-- URI-1 -->
          <rdf:li rdf:resource="http://www.microsoft.com"/>
          <!-- URI-2 -->
          <rdf:li rdf:resource="http://www.google.com"/>
          <!-- URI-3 -->
          <rdf:li rdf:resource="http://www.sun.com"/>
        </rdf:Seq>
      </ore:hasParts>

    </ore:CompoundObject>
  </ore:describes>

</ore:Graph>

</rdf:RDF>